{
    "name":"bright-cloud-studio/isotope-list-ordered-products",
    "description":"Adds to Contao a customized Isotope Order History module that acts as a Product List module showing previously ordered products.",
    "homepage": "http://brightcloudstudio.com/",
    "keywords":["contao", "isotope", "list", "ordered", "products", "bright", "cloud", "studio"],
    "type":"contao-module",
    "license":"LGPL-3.0-or-later",
    "authors":[
        {
            "name":"Bright Cloud Studio",
            "homepage":"https://brightcloudstudio.com"
        }
    ],
    "require":{
        "php": "^8.0",
        "contao/core-bundle": "^4.13",
        "isotope/isotope-core": ">=2.6"
    },
    "require-dev": {
        "contao/manager-plugin": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "Bcs\\": [
                "system/modules/isotope_list_ordered_products/library/Bcs"
            ]
        }
    },
    "extra": {
        "contao": {
            "sources": {
                "system/modules/isotope_list_ordered_products": "system/modules/isotope_list_ordered_products"
            }
        }
    },
    "minimum-stability": "dev"
}
